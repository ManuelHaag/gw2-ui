{
  "name": "gw2-ui-redux",
  "version": "0.0.0",
  "description": "Redux wrapper for the Guild Wars 2 API",
  "keywords": [
    "ajax",
    "axios",
    "guild wars 2",
    "gw2",
    "redux",
    "redux-saga"
  ],
  "homepage": "https://github.com/ManuelHaag/gw2-ui",
  "bugs": "https://github.com/ManuelHaag/gw2-ui/issues",
  "repository": "https://github.com/ManuelHaag/gw2-ui/tree/master/packages/gw2-ui-redux",
  "license": "MIT",
  "author": "Manuel Haag <manuel@haag.online>",
  "files": [
    "lib",
    "es",
    "src"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "prebuild": "npm-run-all clean",
    "build": "npm-run-all -p build:*",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "clean": "rimraf lib es",
    "rebuild": "cross-env NODE_ENV=production npm-run-all build",
    "prewatch": "rimraf es",
    "watch": "npm-run-all \"build:es --watch\""
  },
  "dependencies": {
    "axios": "0.19.2",
    "re-reselect": "2.1.0",
    "redux-actions": "2.6.1",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.2.0",
    "redux": "4.0.0",
    "redux-saga": "0.16.0"
  },
  "peerDependencies": {
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0"
  },
  "sideEffects": false
}
