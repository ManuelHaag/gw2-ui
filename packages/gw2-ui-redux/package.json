{
  "name": "gw2-ui-redux-bulk",
  "version": "0.5.13",
  "description": "Redux wrapper for the Guild Wars 2 API",
  "keywords": [
    "ajax",
    "axios",
    "guild wars 2",
    "gw2",
    "redux"
  ],
  "homepage": "https://github.com/gw2princeps/gw2-ui",
  "bugs": "https://github.com/gw2princeps/gw2-ui/issues",
  "repository": "https://github.com/gw2princeps/gw2-ui/tree/master/packages/gw2-ui-redux",
  "license": "MIT",
  "author": "Manuel Haag <manuel@haag.online>",
  "sideEffects": false,
  "main": "lib/index.js",
  "unpkg": "dist/gw2-ui-redux.min.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "prebuild": "run-s clean",
    "build": "run-p build:* build:*:*",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel --root-mode upward src -d lib --extensions '.js,.jsx,.ts,.tsx'",
    "build:es": "babel --root-mode upward src -d es --extensions '.js,.jsx'",
    "build:umd": "cross-env BABEL_ENV=webpack webpack --mode development -o dist/gw2-ui-redux.js",
    "build:umd:min": "cross-env BABEL_ENV=webpack webpack --mode production -o dist/gw2-ui-redux.min.js",
    "clean": "del-cli lib es dist",
    "prepublish": "run-s build",
    "prewatch": "del-cli es",
    "watch": "run-s \"build:es --watch\""
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.1",
    "function-batch": "^1.1.2"
  },
  "devDependencies": {
    "@babel/cli": "7.15.4",
    "@babel/core": "7.15.5",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/preset-env": "7.15.4",
    "@babel/runtime-corejs3": "7.15.4",
    "axios": "0.21.4",
    "babel-loader": "8.2.2",
    "cross-env": "7.0.3",
    "del-cli": "4.0.1",
    "npm-run-all": "4.1.5",
    "redux": "4.1.1",
    "reselect": "4.0.0",
    "webpack": "5.52.0",
    "webpack-cli": "4.8.0"
  },
  "peerDependencies": {
    "@babel/runtime-corejs3": "^7.15.4",
    "axios": "^0.21.4",
    "redux": "^4.1.1",
    "reselect": "^4.0.0"
  }
}
