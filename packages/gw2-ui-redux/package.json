{
  "name": "gw2-ui-redux",
  "version": "0.4.2",
  "description": "Redux wrapper for the Guild Wars 2 API",
  "keywords": [
    "ajax",
    "axios",
    "guild wars 2",
    "gw2",
    "redux"
  ],
  "homepage": "https://github.com/ManuelHaag/gw2-ui",
  "bugs": "https://github.com/ManuelHaag/gw2-ui/issues",
  "repository": "https://github.com/ManuelHaag/gw2-ui/tree/master/packages/gw2-ui-redux",
  "license": "MIT",
  "author": "Manuel Haag <manuel@haag.online>",
  "files": [
    "lib",
    "es",
    "src"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "unpkg": "dist/gw2-ui-redux.min.js",
  "scripts": {
    "clean": "del-cli lib es dist",
    "prebuild": "run-s clean",
    "build": "run-p build:* build:*:*",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel --root-mode upward src -d lib",
    "build:es": "cross-env babel --root-mode upward src -d es",
    "build:umd": "webpack --mode development -o dist/gw2-ui-redux.js",
    "build:umd:min": "webpack --mode production -o dist/gw2-ui-redux.min.js",
    "prepublish": "run-s build",
    "prewatch": "del-cli es",
    "watch": "run-s \"build:es --watch\""
  },
  "dependencies": {
    "@redux-requests/axios": "^1.0.0",
    "@redux-requests/core": "^1.0.0"
  },
  "devDependencies": {
    "@babel/runtime-corejs3": "7.4.0",
    "axios": "0.16.0",
    "redux": "4.0.0",
    "reselect": "4.0.0"
  },
  "peerDependencies": {
    "@babel/runtime-corejs3": "^7.4.0",
    "axios": "^0.16.0",
    "redux": "^4.0.0",
    "reselect": "^4.0.0"
  },
  "sideEffects": false
}
