{
  "name": "gw2-ui-bulk",
  "version": "0.11.10",
  "description": "React components and API wrapper for Guild Wars 2",
  "keywords": [
    "guild wars 2",
    "gw2",
    "react",
    "redux"
  ],
  "homepage": "https://github.com/gw2princeps/gw2-ui",
  "bugs": "https://github.com/gw2princeps/gw2-ui/issues",
  "repository": "https://github.com/gw2princeps/gw2-ui",
  "license": "MIT",
  "author": "Manuel Haag <manuel@haag.online>",
  "sideEffects": false,
  "main": "lib/index.js",
  "unpkg": "dist/gw2-ui.min.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "prebuild": "run-s clean",
    "build": "run-p build:* build:*:*",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel --root-mode upward src -d lib",
    "build:es": "babel --root-mode upward src -d es",
    "build:umd": "cross-env BABEL_ENV=webpack webpack --mode development -o dist/gw2-ui.js",
    "build:umd:min": "cross-env BABEL_ENV=webpack webpack --mode production -o dist/gw2-ui.min.js",
    "clean": "del-cli lib es dist",
    "prepublish": "run-s build",
    "prewatch": "del-cli es",
    "watch": "run-s \"build:es --watch\""
  },
  "dependencies": {
    "gw2-ui-builder-bulk": "^0.2.4",
    "gw2-ui-components-bulk": "^0.12.8",
    "gw2-ui-redux-bulk": "^0.5.13"
  },
  "devDependencies": {
    "@babel/cli": "7.15.4",
    "@babel/core": "7.15.5",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/preset-env": "7.15.4",
    "@babel/preset-react": "7.14.5",
    "@babel/runtime-corejs3": "7.15.4",
    "babel-loader": "8.2.2",
    "cross-env": "7.0.3",
    "del-cli": "4.0.1",
    "file-loader": "6.2.0",
    "npm-run-all": "4.1.5",
    "prop-types": "15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "7.2.5",
    "redux": "4.1.1",
    "reselect": "4.0.0",
    "url-loader": "4.1.1",
    "webpack": "5.52.0",
    "webpack-cli": "4.8.0"
  },
  "peerDependencies": {
    "@babel/runtime-corejs3": "^7.15.4",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "reselect": "^4.0.0"
  }
}
