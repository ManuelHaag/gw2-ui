{
  "name": "gw2-ui-bulk",
  "version": "0.10.16",
  "description": "React components and API wrapper for Guild Wars 2",
  "keywords": [
    "guild wars 2",
    "gw2",
    "react",
    "redux"
  ],
  "homepage": "https://github.com/gw2princeps/gw2-ui",
  "bugs": "https://github.com/gw2princeps/gw2-ui/issues",
  "repository": "https://github.com/gw2princeps/gw2-ui",
  "license": "MIT",
  "author": "Manuel Haag <manuel@haag.online>",
  "sideEffects": false,
  "main": "lib/index.js",
  "unpkg": "dist/gw2-ui.min.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "prebuild": "run-s clean",
    "build": "run-p build:* build:*:*",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel --root-mode upward src -d lib",
    "build:es": "babel --root-mode upward src -d es",
    "build:umd": "cross-env BABEL_ENV=webpack webpack --mode development -o dist/gw2-ui.js",
    "build:umd:min": "cross-env BABEL_ENV=webpack webpack --mode production -o dist/gw2-ui.min.js",
    "clean": "del-cli lib es dist",
    "prepublish": "run-s build",
    "prewatch": "del-cli es",
    "watch": "run-s \"build:es --watch\""
  },
  "dependencies": {
    "@redux-requests/react": "1.0.0",
    "gw2-ui-builder-bulk": "^0.2.3",
    "gw2-ui-components-bulk": "^0.11.6",
    "gw2-ui-redux-bulk": "^0.5.8"
  },
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@babel/runtime-corejs3": "7.4.0",
    "@redux-requests/core": "1.1.1",
    "babel-loader": "8.1.0",
    "cross-env": "7.0.2",
    "del-cli": "3.0.1",
    "file-loader": "6.1.0",
    "npm-run-all": "4.1.5",
    "prop-types": "15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "7.2.0",
    "redux": "4.0.5",
    "reselect": "4.0.0",
    "url-loader": "4.1.1",
    "webpack": "5.0.0",
    "webpack-cli": "4.0.0"
  },
  "peerDependencies": {
    "@babel/runtime-corejs3": "^7.4.0",
    "@redux-requests/core": "^1.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "reselect": "^4.0.0"
  }
}
