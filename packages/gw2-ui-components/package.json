{
  "name": "gw2-ui-components",
  "version": "0.10.1",
  "description": "React components for Guild Wars 2",
  "keywords": [
    "guild wars 2",
    "gw2",
    "react",
    "theme-ui"
  ],
  "homepage": "https://github.com/ManuelHaag/gw2-ui",
  "bugs": "https://github.com/ManuelHaag/gw2-ui/issues",
  "repository": "https://github.com/ManuelHaag/gw2-ui/tree/master/packages/gw2-ui-components",
  "license": "MIT",
  "author": "Manuel Haag <manuel@haag.online>",
  "sideEffects": false,
  "main": "lib/index.js",
  "unpkg": "dist/gw2-ui-components.min.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "prebuild": "run-s clean",
    "build": "run-p build:* build:*:*",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel --root-mode upward src -d lib --copy-files",
    "build:es": "cross-env babel --root-mode upward src -d es --copy-files",
    "build:umd": "webpack --mode development -o dist/gw2-ui-components.js",
    "build:umd:min": "webpack --mode production -o dist/gw2-ui-components.min.js",
    "clean": "del-cli lib es dist",
    "prepublish": "run-s build",
    "prewatch": "del-cli es",
    "watch": "run-s \"build:es --watch\""
  },
  "dependencies": {
    "@theme-ui/core": "^0.3.1",
    "@tippyjs/react": "^4.2.0",
    "classnames": "^2.2.6",
    "lodash.camelcase": "^4.3.0",
    "react-resize-aware": "^3.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-transform-react-jsx": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "@babel/runtime-corejs3": "7.4.0",
    "@emotion/babel-plugin-jsx-pragmatic": "0.1.5",
    "prop-types": "15.7.2",
    "react": "16.8.0",
    "react-dom": "16.8.0"
  },
  "peerDependencies": {
    "@babel/runtime-corejs3": "^7.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  }
}
